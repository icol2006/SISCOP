            <td>
                @Html.DisplayFor(modelItem => item.estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.encargado)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id_etapa }) |
                @Html.ActionLink("Details", "Details", new { id = item.id_etapa }) |
